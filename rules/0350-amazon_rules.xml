<!-- ID: 80200 - 80299 -->
<group name="amazon,">
    <rule id="80200" level="0">
        <decoded_as>json</decoded_as>
        <field name="_log_type">AWS</field>
        <description>Amazon alerts.</description>
    </rule>

    <rule id="80201" level="0">
        <if_sid>80200</if_sid>
        <field name="eventSource">ec2.amazonaws.com|elasticloadbalancing.amazonaws.com</field>
        <description>Amazon EC2 alerts.</description>
        <group>amazon-ec2,</group>
    </rule>

    <rule id="80202" level="0">
        <if_sid>80200</if_sid>
        <field name="eventSource">iam.amazonaws.com|signin.amazonaws.com|kms.amazonaws.com</field>
        <description>Amazon IAM alerts.</description>
        <group>amazon-iam,</group>
    </rule>

    <rule id="80203" level="0">
        <if_sid>80200</if_sid>
        <field name="eventSource">s3.amazonaws.com</field>
        <description>Amazon s3 alerts.</description>
        <group>amazon-s3,</group>
    </rule>


    <!-- ID for amazon events 80200-80300 -->

</group>
